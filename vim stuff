my vimrc
syntax on
colorscheme murphy 	" I also like elflord and industry
set number		" show line numbers on the left
set cursorline
set cursorcolumn
set incsearch
set ignorecase
set smartcase
set showmode
set showmatch

set laststatus=2
set statusline=%F\ %y\ %=\ %4v,%4l\ %3p%%\ of\ %L\ lines
hi StatusLine cterm=none

inoremap jj <esc>
